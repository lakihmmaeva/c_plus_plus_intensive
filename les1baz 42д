#include <iostream>
using namespace std;
int main()
{
    float a, a2, a4, a5, a10, a15;
    cout << "Введите a" << endl;
    cin >> a;
    a2 = a*a;
    a4 = a2*a2;
    a5 = a4*a;
    a10 = a5*a5;
    a15 = a10*a5;
    a = a15*a2;
    cout << "a^2 = " << a2 << endl;
    cout << "a^5 = " << a5 << endl;
    cout << "a^17 = " << a << endl;
    return 0;
}
