#include <iostream> 
using namespace std;

int main() { 
    int A, B; 
    cout << "Enter two integers A and B (A < B): "; 
    cin >> A >> B; 
    int sum = 0; 
    for (int i = A; i <= B; i++) { 
        sum += i; 
    }
    cout << "The sum of all integers from " << A << " to " << B << " is " << sum << endl; 
    return 0; 
}
