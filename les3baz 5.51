#include <iostream>
using namespace std;

int main() {
    int n;
    double sum = 0, average;
    
    cout << "Enter the count of numbers: ";
    cin >> n;
    
    cout << "Enter " << n << " numbers: ";
    for (int i = 0; i < n; ++i) {
        double num;
        cin >> num;
        sum += num;
    }
    
    average = sum / n;
    
    cout << "Average: " << average << endl;
    
    return 0;
}
