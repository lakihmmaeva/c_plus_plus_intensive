#include <iostream>
#include <cmath>
using namespace std;

void printSquares(int n) {
    int number = 1;
    
    while (number * number <= n) {
        cout << number * number << " ";
        number++;
    }
    
    cout << endl;
}

int main() {
    int n;
    cout << "Введите значение n: ";
    cin >> n;
    
    cout << "Квадраты чисел, не превышающие " << n << ": ";
    printSquares(n);
    
    return 0;
}
