#include <iostream>
using namespace std;
int main()
{
	int a, b, l;
	cout << "Введите первое (большее) число a (учитываем, что все числа положительные) " << endl;
	cin >> a;
	cout << "Введите первое (меньшее) число b (учитываем, что все числа положительные) " << endl;
	cin >> b;
	l = a/b;
	cout << "В отрезок А помещается " << l << " отрезка(ов) В" << endl;
	return 0;
}
