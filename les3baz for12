#include <iostream>
using namespace std;

int main() {
    int N;
    double product = 1.1;
    cout << "Enter an integer N: ";
    cin >> N;
    for(int i = 2; i <= N; i++) {
        product *= 1.0 + (i - 1) / 10.0;
    }
    cout << "Product of 1.1 * 1.2 * 1.3 * ... * (N+1)/10 is: " << product << endl;
    return 0;
}
