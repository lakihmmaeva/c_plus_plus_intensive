#include <iostream>
using namespace std;

int main() {
  float A, B;
  cout << "Введите значение переменной A: ";
  cin >> A;
  cout << "Введите значение переменной B: ";
  cin >> B;

  if (A > B) {
    float temp = A;
    A = B;
    B = temp;
  }

  cout << "Новое значение переменной A: " << A << endl;
  cout << "Новое значение переменной B: " << B << endl;

  return 0;
}
