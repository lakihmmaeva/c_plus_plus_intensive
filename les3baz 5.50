#include <iostream>
using namespace std;

int main() {
    double a1, a2, a3, a4, a5, a6, a7, a8, a9, a10;
    double sum, average;
    
    cout << "Enter 10 numbers: ";
    cin >> a1 >> a2 >> a3 >> a4 >> a5 >> a6 >> a7 >> a8 >> a9 >> a10;
    
    sum = a1 + a2 + a3 + a4 + a5 + a6 + a7 + a8 + a9 + a10;
    average = sum / 10;
    
    cout << "Average: " << average << endl;
    
    return 0;
}
