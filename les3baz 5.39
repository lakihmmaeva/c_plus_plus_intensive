#include <iostream>
using namespace std;

int main() {
    int a[10]; // создаем массив из 10 элементов
    int sum = 0; // сумма чисел

    // вводим числа с клавиатуры и считаем сумму
    for (int i = 0; i < 10; i++) {
        cout << "Введите число a" << i+1 << ": ";
        cin >> a[i];
        sum += a[i];
    }

    // выводим результат
    cout << "Сумма чисел: " << sum << endl;

    return 0;
}
