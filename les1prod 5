#include <iostream>
#include <cmath>

using namespace std;

int main() {
    const double g = 9.81; // ускорение свободного падения
    double H, U, T, S; // высота, скорость, время, расстояние

    cout << "Введите высоту самолета: ";
    cin >> H;
    cout << "Введите скорость самолета: ";
    cin >> U;

    T = sqrt(2 * H / g); // вычисляем время полета бомбы
    S = U * T; // вычисляем расстояние до объекта

    cout << "Подлетное расстояние до объекта: " << S << endl;

    return 0;
}
