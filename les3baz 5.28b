#include <iostream>
using namespace std;

int main() {
    int a, product = 1;
    cout << "Enter a value between 1 and 20: ";
    cin >> a;
    for (int i = a; i <= 20; i++) {
        product *= i;
    }
    cout << "Product of integers from " << a << " to 20: " << product << endl;
    return 0;
}
