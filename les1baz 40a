#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    float t, a, b, c;
    cout << "Введите a" << endl;
    cin >> a;
    cout << "Введите b" << endl;
    cin >> b;
    cout << "Введите c" << endl;
    cin >> c;
    t = a;
    a = b;
    b = c;
    c = t;
    cout << "a = " << a << endl;
    cout << "b = " << b << endl;
    cout << "c = " << c << endl;
    return 0;
}
