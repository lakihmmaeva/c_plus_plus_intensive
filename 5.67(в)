#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Введите число n: ";
    cin >> n;

    int f1 = 1, f2 = 1, sum = 2;
    for (int i = 3; i <= n; i++) {
        int f = f1 + f2;
        sum += f;
        f1 = f2;
        f2 = f;
    }

    if (sum % 2 == 0) {
        cout << "Сумма первых " << n << " членов последовательности Фибоначчи является четным числом.";
    } else {
        cout << "Сумма первых " << n << " членов последовательности Фибоначчи не является четным числом.";
    }

    return 0;
}
