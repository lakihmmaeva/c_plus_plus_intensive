#include <iostream>
using namespace std;

int main() {
    int num1, num2;

    cout << "Введите первое число: ";
    cin >> num1;

    cout << "Введите второе число: ";
    cin >> num2;

    if (num1 > num2) {
        cout << num1 << " больше " << num2 << endl;
    } else {
        cout << num2 << " больше " << num1 << endl;
    }

    return 0;
}
